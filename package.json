{
  "name": "@himenon/openapi-typescript-code-generator-play",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://github.com/Himenon/tiny-react-app-template#readme",
  "bugs": "https://github.com/Himenon/tiny-react-app-template/issues",
  "repository": {
    "type": "git",
    "url": "git@github.com:Himenon/tiny-react-app-template.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc && vite build",
    "clean": "rimraf dist lib",
    "commit": "npx git-cz",
    "copy": "ts-node -P tsconfig.cli.json ./gen-docs.ts",
    "depcruise": "depcruise --validate .dependency-cruiser.js src",
    "depcruise:html": "depcruise --validate --output-type err-html -f dependency-report.html .dependency-cruiser.js src",
    "deploy": "pnpm build && pnpm publish:site",
    "dev": "vite",
    "format": "pnpm lint --fix && prettier --write \"**/*.{ts,tsx,js,jsx,json,yml,yaml,html,md}\"",
    "lint": "eslint -c ./.eslintrc.js 'src/**/*.{ts,tsx}'",
    "lint:fix": "pnpm lint --fix",
    "prettier": "prettier --config .prettierrc --write src/**/*",
    "preview": "vite preview",
    "publish:site": "ts-node -P ./scripts/tsconfig.cli.json ./scripts/publish.ts",
    "serve": "serve dist",
    "start": "vite",
    "test": "echo 'SKIP'",
    "test:jest": "jest -c ./jest.config.js",
    "test:jest:watch": "pnpm test:jest --watch",
    "version_up": "lerna version --yes"
  },
  "dependencies": {
    "@himenon/openapi-typescript-code-generator": "0.18.3",
    "@monaco-editor/react": "^4.4.4",
    "js-yaml": "^4.1.0",
    "lz-string": "1.4.4",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "styled-components": "5.3.5",
    "typescript": "4.6.4"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.4",
    "@commitlint/config-conventional": "16.2.4",
    "@swc/core": "^1.2.177",
    "@swc/helpers": "^0.3.13",
    "@swc/jest": "^0.2.20",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "27.5.0",
    "@types/js-yaml": "4.0.5",
    "@types/lz-string": "1.3.34",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "5.1.3",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "@vitejs/plugin-react": "^1.3.2",
    "cross-env": "7.0.3",
    "dependency-cruiser": "11.7.0",
    "eslint": "8.14.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.5.0",
    "fs-extra": "^10.1.0",
    "jest": "28.0.3",
    "lerna": "4.0.0",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "process": "^0.11.10",
    "serve": "13.0.2",
    "sort-package-json": "1.57.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "28.0.1",
    "ts-node": "10.7.0",
    "vite": "^2.9.8"
  }
}
