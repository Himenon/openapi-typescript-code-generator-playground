{
  "name": "@himenon/openapi-typescript-code-generator-play",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://github.com/Himenon/tiny-react-app-template#readme",
  "bugs": "https://github.com/Himenon/tiny-react-app-template/issues",
  "repository": {
    "type": "git",
    "url": "git@github.com:Himenon/tiny-react-app-template.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc && vite build",
    "clean": "rimraf dist lib",
    "commit": "npx git-cz",
    "copy": "ts-node -P tsconfig.cli.json ./gen-docs.ts",
    "depcruise": "depcruise --validate .dependency-cruiser.js src",
    "depcruise:html": "depcruise --validate --output-type err-html -f dependency-report.html .dependency-cruiser.js src",
    "deploy": "pnpm build && pnpm publish:site",
    "dev": "vite",
    "format": "pnpm lint --fix && prettier --write \"**/*.{ts,tsx,js,jsx,json,yml,yaml,html,md}\"",
    "lint": "eslint -c ./.eslintrc.js 'src/**/*.{ts,tsx}'",
    "lint:fix": "pnpm lint --fix",
    "prettier": "prettier --config .prettierrc --write src/**/*",
    "preview": "vite preview",
    "publish:site": "ts-node -P ./scripts/tsconfig.cli.json ./scripts/publish.ts",
    "serve": "serve dist",
    "start": "vite",
    "test": "echo 'SKIP'",
    "test:jest": "jest -c ./jest.config.js",
    "test:jest:watch": "pnpm test:jest --watch",
    "version_up": "lerna version --yes"
  },
  "dependencies": {
    "@himenon/openapi-typescript-code-generator": "0.27.4",
    "@monaco-editor/react": "^4.6.0",
    "js-yaml": "^4.1.0",
    "lz-string": "1.5.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "6.1.8",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "18.6.0",
    "@commitlint/config-conventional": "18.6.0",
    "@swc/core": "^1.3.107",
    "@swc/helpers": "^0.5.3",
    "@swc/jest": "^0.2.34",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "29.5.12",
    "@types/js-yaml": "4.0.9",
    "@types/lz-string": "1.3.34",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.52",
    "@types/react-dom": "^18.2.18",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cross-env": "7.0.3",
    "dependency-cruiser": "16.1.0",
    "eslint": "8.56.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "fs-extra": "^11.2.0",
    "jest": "29.7.0",
    "lerna": "8.0.2",
    "lint-staged": "15.2.1",
    "prettier": "3.2.4",
    "process": "^0.11.10",
    "serve": "14.2.1",
    "sort-package-json": "2.7.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "vite": "^5.0.12"
  }
}
