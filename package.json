{
  "name": "@himenon/openapi-typescript-code-generator-play",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://github.com/Himenon/tiny-react-app-template#readme",
  "bugs": "https://github.com/Himenon/tiny-react-app-template/issues",
  "repository": {
    "type": "git",
    "url": "git@github.com:Himenon/tiny-react-app-template.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "clean": "rimraf dist lib",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "commit": "npx git-cz",
    "depcruise": "depcruise --validate .dependency-cruiser.js src",
    "depcruise:html": "depcruise --validate --output-type err-html -f dependency-report.html .dependency-cruiser.js src",
    "deploy": "yarn build && yarn publish:site",
    "format": "yarn lint --fix && prettier --write \"**/*.{ts,tsx,js,jsx,json,yml,yaml,html,md}\"",
    "lint": "eslint -c ./.eslintrc.js 'src/**/*.{ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier --config .prettierrc --write src/**/*",
    "publish:site": "ts-node -P ./scripts/tsconfig.cli.json ./scripts/publish.ts",
    "serve": "serve dist",
    "test": "yarn test:jest && yarn depcruise",
    "test:jest": "jest -c ./jest.config.json",
    "test:jest:watch": "yarn test:jest --watch",
    "version_up": "lerna version --yes"
  },
  "dependencies": {
    "@emotion/core": "11.0.0",
    "@emotion/styled": "11.8.1",
    "@himenon/openapi-typescript-code-generator": "0.18.0",
    "@monaco-editor/react": "^4.4.4",
    "@rebass/emotion": "3.2.0",
    "acorn": "8.7.1",
    "acorn-jsx": "5.3.2",
    "astring": "1.8.3",
    "estree-walker": "3.0.1",
    "js-yaml": "^4.1.0",
    "lz-string": "1.4.4",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "pretty": "2.0.0",
    "prism-react-renderer": "^1.3.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-live": "2.4.1",
    "react-monaco-editor": "0.48.0",
    "react-router-dom": "6.3.0",
    "typescript": "4.6.4"
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@babel/preset-env": "7.17.10",
    "@commitlint/cli": "16.2.4",
    "@commitlint/config-conventional": "16.2.4",
    "@types/acorn": "4.0.6",
    "@types/astring": "1.7.0",
    "@types/gh-pages": "3.2.1",
    "@types/jest": "27.5.0",
    "@types/js-yaml": "4.0.5",
    "@types/lz-string": "1.3.34",
    "@types/node": "^17.0.31",
    "@types/pretty": "2.0.1",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "10.4.7",
    "commitizen": "4.2.4",
    "conventional-changelog-angular-all": "1.7.0",
    "core-js": "^3.22.4",
    "cross-env": "7.0.3",
    "dependency-cruiser": "11.7.0",
    "dts-bundle": "0.7.3",
    "eslint": "8.14.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.5.0",
    "fibers": "5.0.1",
    "gh-pages": "3.2.3",
    "husky": "7.0.4",
    "jest": "28.0.3",
    "lerna": "4.0.0",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "regenerator-runtime": "0.13.9",
    "sass": "1.51.0",
    "serve": "13.0.2",
    "sort-package-json": "1.57.0",
    "styled-components": "5.3.5",
    "ts-jest": "28.0.1",
    "ts-node": "10.7.0",
    "vite": "^2.9.8"
  }
}
